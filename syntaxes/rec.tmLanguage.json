{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "rec",
	"patterns": [
		{
			"include": "#keyword-rec"
		},
        {
            "include": "#comments-rec"
        }

	],
	"repository": {
		"keywords-rec": {
			"patterns": [{
				"name": "keyword.control.stol",
				"match": "\\b(?i)()\\b"
			}]
		},
		"comments-rec": {
            "name": "comment.block.documentation.rec",
			"begin": "/\\*",
			"end": "\\*/"
		}
	},
	"scopeName": "source.rec"
}